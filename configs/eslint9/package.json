{
  "name": "@compose/eslint9-config",
  "type": "module",
  "version": "1.0.7",
  "license": "LGPL-2.1-or-later",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./defaults": {
      "types": "./dist/defaults/index.d.ts",
      "import": "./dist/defaults/index.js",
      "require": "./dist/defaults/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix",
    "build-c": "vite build",
    "type-check": "vue-tsc --noEmit",
    "build": "run-s type-check lint build-c"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "catalog:eslint",
    "@eslint/js": "catalog:eslint",
    "@unocss/eslint-config": "catalog:unocss",
    "@vue/eslint-config-prettier": "catalog:eslint",
    "@vue/eslint-config-typescript": "catalog:eslint",
    "eslint": "catalog:eslint",
    "eslint-plugin-prettier": "catalog:eslint",
    "eslint-plugin-vue": "catalog:eslint",
    "prettier": "catalog:",
    "typescript-eslint": "catalog:eslint"
  }
}
