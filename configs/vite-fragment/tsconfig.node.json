{
  "extends": ["@tsconfig/node22/tsconfig.json", "@compose/tsconfig/tsconfig.json"],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": false,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"]
  }
}
