{
  "name": "@compose/tsconfig",
  "version": "1.0.5",
  "description": "TypeScript configuration for compose projects",
  "author": "TrueNine",
  "license": "LGPL-2.1-or-later",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./tsconfig.json",
      "import": "./tsconfig.json",
      "require": "./tsconfig.json"
    },
    "./*": "./*",
    "./tsconfig.json": "./tsconfig.json"
  },
  "main": "tsconfig.json",
  "files": [
    "*.json",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:types",
    "@vue/tsconfig": "catalog:vue",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:types",
    "@vue/tsconfig": "catalog:vue"
  }
}
