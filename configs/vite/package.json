{
  "name": "@truenine/config-vite",
  "type": "module",
  "version": "2026.10107.100",
  "description": "vite config fragment",
  "author": "TrueNine",
  "license": "LGPL-2.1-or-later",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./excludes\\index": {
      "import": "./dist/excludes\\index.js",
      "require": "./dist/excludes\\index.cjs"
    },
    "./externals\\index": {
      "import": "./dist/externals\\index.js",
      "require": "./dist/externals\\index.cjs"
    },
    "./lib\\index": {
      "import": "./dist/lib\\index.js",
      "require": "./dist/lib\\index.cjs"
    },
    "./vite-plugin-dts\\index": {
      "import": "./dist/vite-plugin-dts\\index.js",
      "require": "./dist/vite-plugin-dts\\index.cjs"
    },
    "./vite-plugin-package-json\\index": {
      "import": "./dist/vite-plugin-package-json\\index.js",
      "require": "./dist/vite-plugin-package-json\\index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./excludes/index": {
        "import": "./dist/excludes/index.js",
        "require": "./dist/excludes/index.cjs"
      },
      "./externals/index": {
        "import": "./dist/externals/index.js",
        "require": "./dist/externals/index.cjs"
      },
      "./lib/index": {
        "import": "./dist/lib/index.js",
        "require": "./dist/lib/index.cjs"
      },
      "./vite-plugin-dts/index": {
        "import": "./dist/vite-plugin-dts/index.js",
        "require": "./dist/vite-plugin-dts/index.cjs"
      },
      "./vite-plugin-package-json/index": {
        "import": "./dist/vite-plugin-package-json/index.js",
        "require": "./dist/vite-plugin-package-json/index.cjs"
      },
      "./*": "./*"
    }
  },
  "scripts": {
    "lintfix": "eslint --fix",
    "build-c": "vite build",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "check": "run-p typecheck lintfix",
    "build": "tsdown"
  },
  "peerDependencies": {
    "rollup": "catalog:rollup",
    "typescript": "catalog:",
    "vite": "catalog:vite",
    "vite-plugin-dts": "catalog:vite"
  },
  "dependencies": {
    "rollup": "catalog:rollup",
    "typescript": "catalog:",
    "vite": "catalog:vite",
    "vite-plugin-dts": "catalog:vite"
  }
}
