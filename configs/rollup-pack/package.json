{
  "name": "@compose/rollup-pack-config",
  "version": "0.0.33",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "prepare": "pnpm build",
    "build": "pnpm rollup --config rollup.config.ts --configPlugin typescript",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm -C dist pub"
  },
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./*": "./*"
  }
}