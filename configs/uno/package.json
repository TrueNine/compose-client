{
  "name": "@compose/config-uno",
  "type": "module",
  "version": "1.0.1",
  "description": "uno css code snippets",
  "author": "TrueNine",
  "license": "private",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
