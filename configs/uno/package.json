{
  "name": "@compose/config-uno",
  "version": "0.1.180",
  "description": "uno css code snippets",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build": "run-s type-check build-c",
    "build-c": "vite build",
    "type-check": "vue-tsc --noEmit",
    "pub": "run-s  build pub-c",
    "pub-c": "pnpm publish --no-git-checks"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@unocss/cli": "^0.61.4",
    "@unocss/core": "^0.61.4",
    "@unocss/postcss": "^0.61.4",
    "@unocss/transformer-compile-class": "^0.61.4",
    "@unocss/transformer-directives": "^0.61.4",
    "unocss": "^0.61.4",
    "unocss-preset-weapp": "^0.61.0"
  },
  "peerDependencies": {
    "@unocss/cli": "^0.61.4",
    "@unocss/core": "^0.61.4",
    "@unocss/postcss": "^0.61.4",
    "@unocss/transformer-compile-class": "^0.61.4",
    "@unocss/transformer-directives": "^0.61.4",
    "unocss": "^0.61.4",
    "unocss-preset-weapp": "^0.61.0"
  }
}
