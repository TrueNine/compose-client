    "build-c": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@unocss/transformer-attributify-jsx": "catalog:unocss",
    "@unocss/transformer-compile-class": "catalog:unocss",
    "unocss": "catalog:unocss"
  },
  "devDependencies": {
    "@compose/config-vite": "workspace:^"
