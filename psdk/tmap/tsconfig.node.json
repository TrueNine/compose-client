{
  "extends": ["@tsconfig/node22/tsconfig.json", "@truenine/tsconfig/tsconfig.json"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"],
    "noEmit": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "tsdown.config.*",
    "env.d.ts",
    "vite.config.ts"
  ]
}
