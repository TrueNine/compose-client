{
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    ".env.example": ".env*",
    "*.ts": "$(capture).js, $(capture).d.ts, $(capture).*.ts, $(capture).*.js",
    "README.md": "LICENSE, .editorconfig",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, .npmignore, .npmrc",
    ".env": ".env.local, .env.*.local",
    "tsconfig.json": "tsconfig.*.json, env.d.ts, tsconfig.*.tsbuildinfo, tsconfig.tsbuildinfo",
    "AGENTS.md": "GEMINI.md, WARP.md, CLAUDE.md",
    ".gitignore": ".gitattributes, .cursorignore, .qoderignore, .warpindexignore",
    "tsdown.config.ts": "vitest.config.ts, vite.config.ts, eslint.config.ts",
    "next.config.ts": "vitest.config.ts, vite.config.ts, eslint.config.ts, postcss.config.mjs, next-env.d.ts"
  },
  "workbench.sideBar.location": "left",
  "diffEditor.renderSideBySide": false,
  "editor.rulers": [
    2,
    4,
    80,
    160
  ],
  "editor.fontFamily": "JetBrains Mono",
  "chatgpt.openOnStartup": true
}
