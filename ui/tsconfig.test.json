{
  "extends": "@truenine/tsconfig/tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.test.tsbuildinfo",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "baseUrl": ".",
    "moduleResolution": "Bundler",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["vitest/globals", "@vue/test-utils", "jsdom", "node", "./.types/imports-auto", "./.types/imports-comp"]
  },
  "include": [
    "src/**/__tests__/**/*.{spec,test}.{ts,tsx}",
    "src/**/*.{spec,test}.{ts,tsx}",
    "src/**/*",
    "common/**/*",
    "unplugin/**/*",
    ".types/**/*",
    "env.d.ts"
  ],
  "vueCompilerOptions": {
    "target": 3.3
  }
}
