{
  "name": "@compose/ui",
  "type": "module",
  "version": "1.1.292",
  "license": "LGPL-2.1-or-later",
  "homepage": "https://github.com/TrueNine",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/TrueNine/compose-client",
    "directory": "meta-ui-web"
  },
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css",
    "*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./unplugin": {
      "types": "./dist/unplugin/index.d.ts",
      "import": "./dist/unplugin/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./*": "./*",
    "./**": "./**",
    "./unocss.css": "./dist/unocss.css",
    "./index.css": "./dist/index.css",
    "./base.css": "./dist/base.css",
    "./dist/*": "./dist/*"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "dev": "vite",
    "lint": "eslint --fix",
    "build-c": "vite build",
    "build-g": "gulp -f __build-src__/index.js",
    "type-check": "vue-tsc --noEmit",
    "build": "run-s type-check lint test build-c build-g"
  },
  "peerDependencies": {
    "@compose/shared": "workspace:^",
    "@compose/types": "workspace:^",
    "@compose/vue": "workspace:^",
    "@date-io/dayjs": "catalog:",
    "@iconify-json/ant-design": "catalog:iconify",
    "@iconify-json/flag": "catalog:iconify",
    "@iconify-json/fluent": "catalog:iconify",
    "@iconify-json/ion": "catalog:iconify",
    "@iconify-json/mdi": "catalog:iconify",
    "@iconify-json/simple-icons": "catalog:iconify",
    "@mdi/font": "catalog:vuetify",
    "@varlet/ui": "catalog:",
    "@vee-validate/yup": "catalog:vee-validate",
    "@vee-validate/zod": "catalog:vee-validate",
    "@vitejs/plugin-vue": "catalog:vite",
    "@vitejs/plugin-vue-jsx": "catalog:vite",
    "@vueuse/core": "catalog:vueuse",
    "dayjs": "catalog:",
    "element-plus": "catalog:",
    "unocss": "catalog:unocss",
    "vee-validate": "catalog:vee-validate",
    "vite-plugin-vuetify": "catalog:vuetify",
    "vue": "catalog:vue",
    "vuetify": "catalog:vuetify",
    "yup": "catalog:vee-validate",
    "zod": "catalog:vee-validate"
  },
  "devDependencies": {
    "@compose/config-uno": "workspace:^",
    "@compose/config-vite": "workspace:^",
    "@types/gulp": "catalog:gulp",
    "@types/gulp-autoprefixer": "catalog:gulp",
    "@types/gulp-postcss": "catalog:gulp",
    "@types/gulp-sass": "catalog:gulp",
    "@types/jsdom": "catalog:vitest",
    "@unocss/cli": "catalog:unocss",
    "@unocss/postcss": "catalog:unocss",
    "@vue/test-utils": "catalog:vue",
    "@vue/tsconfig": "catalog:vue",
    "autoprefixer": "catalog:",
    "cssnano": "catalog:cssnano",
    "cssnano-preset-advanced": "catalog:cssnano",
    "gulp": "catalog:gulp",
    "gulp-postcss": "catalog:gulp",
    "gulp-sass": "catalog:gulp",
    "jsdom": "catalog:vitest",
    "sass": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-fonts": "catalog:",
    "unplugin-vue-components": "catalog:",
    "unplugin-vue-router": "catalog:",
    "vite-plugin-vue-devtools": "catalog:vite",
    "vue-router": "catalog:vue"
  }
}
