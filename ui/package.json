{
  "name": "@compose/ui",
  "type": "module",
  "version": "1.1.284",
  "homepage": "https://github.com/TrueNine",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/TrueNine/compose-client",
    "directory": "meta-ui-web"
  },
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css",
    "*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./unplugin": {
      "types": "./dist/unplugin/index.d.ts",
      "import": "./dist/unplugin/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.js"
    },
    "./*": "./*",
    "./**": "./**",
    "./unocss.css": "./unocss.css",
    "./dist/*": "./dist/*"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "vitest run",
    "dev": "vite",
    "lint": "eslint --fix",
    "build-c": "vite build",
    "build-g": "gulp -f __build-src__/index.js",
    "type-check": "vue-tsc --noEmit",
    "build": "run-s type-check lint test build-c build-g",
    "pub-c": "pnpm --dir dist pub",
    "pub": "run-s build pub-c"
  },
  "peerDependencies": {
    "@varlet/ui": "*",
    "@vee-validate/yup": "*",
    "@vee-validate/zod": "*",
    "@vueuse/core": "*",
    "dayjs": "*",
    "element-plus": "*",
    "naive-ui": "*",
    "vee-validate": "*",
    "vue": "*",
    "vuetify": "*"
  }
}
