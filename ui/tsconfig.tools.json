{
  "extends": "@truenine/tsconfig/tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tools.tsbuildinfo",
    "lib": ["ESNext", "DOM"],
    "types": ["node", "vite/client"],
    "allowImportingTsExtensions": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noEmit": true
  },
  "include": [
    "env.d.ts",
    "vite.config.*",
    "vitest.config.*",
    "uno.config.*",
    "eslint.config.*",
    "tsdown.config.*",
    "*.config.*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "playground/**/*"
  ]
}
