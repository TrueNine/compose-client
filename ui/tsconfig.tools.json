{
  "extends": "@truenine/tsconfig/tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tools.tsbuildinfo",
    "lib": ["ESNext"],
    "moduleResolution": "Bundler",
    "types": ["node"],
    "allowImportingTsExtensions": true
  },
  "include": [
    "env.d.ts",
    "vite.config.*",
    "vitest.config.*",
    "uno.config.*",
    "eslint.config.*",
    "tsdown.config.*",
    "*.config.*"
  ]
}
