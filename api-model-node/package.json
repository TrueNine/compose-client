{
  "name": "@compose/api-model-node",
  "version": "1.1.3",
  "description": "yan100 client nodejs kit",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build": "pnpm rollup --config rollup.config.ts --configPlugin typescript",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm -C dist pub"
  },
  "devDependencies": {
    "@compose/compose-types": "workspace:^"
  },
  "dependencies": {
    "@compose/compose-types": "workspace:^",
    "@compose/api-model": "workspace:^"
  },
  "peerDependencies": {
    "@compose/compose-types": "workspace:^",
    "@compose/api-model": "workspace:^"
  },
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": {},
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./*": "./*"
  }
}
