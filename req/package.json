{
  "name": "@compose/req",
  "version": "0.0.8",
  "description": "requester",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build": "pnpm rollup -c",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm publish --no-git-checks"
  },
  "peerDependencies": {
    "@compose/api-model": "workspace:^",
    "@vueuse/core": "^10.2.1",
    "@types/sockjs-client": "^1.5.1",
    "@types/stompjs": "^2.3.5",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3"
  },
  "type": "module",
  "types": "types/index.d.ts",
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./*": "./*"
  }
}
