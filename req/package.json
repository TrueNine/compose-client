{
  "name": "@compose/req",
  "version": "0.0.14",
  "description": "requester",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build": "pnpm rollup --config rollup.config.ts --configPlugin typescript",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm -C dist pub"
  },
  "peerDependencies": {
    "@compose/api-model": "workspace:^",
    "@stomp/stompjs": "^7.0.0",
    "@types/sockjs-client": "^1.5.1",
    "@types/stompjs": "^2.3.5",
    "@vueuse/core": "^10.2.1",
    "sockjs-client": "^1.6.1",
    "stomp-client": "^0.9.0",
    "stompjs": "^2.3.3",
    "ws": "^8.13.0"
  },
  "type": "module",
  "types": "types/index.d.ts",
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./*": "./*"
  },
  "devDependencies": {
    "@types/ws": "^8.5.6"
  }
}
