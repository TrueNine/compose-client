{
  "name": "@compose/atomic-css-uno",
  "version": "0.1.134",
  "description": "uno css code snippets",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build": "pnpm rollup --config rollup.config.ts --configPlugin typescript",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm -C dist pub"
  },
  "peerDependencies": {
    "@compose/atomic-css-theme": "workspace:^",
    "@unocss/core": "^0.55.7",
    "@unocss/transformer-compile-class": "^0.55.7",
    "@unocss/transformer-directives": "^0.55.7",
    "unocss": "^0.55.7",
    "unocss-preset-weapp": "^0.53.7"
  },
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./*": "./*"
  }
}
