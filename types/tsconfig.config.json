{
  "extends": "@tsconfig/node22/tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.config.tsbuildinfo",
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"],
    "noEmit": false,
    "isolatedDeclarations": false
  },
  "include": [
    "env.d.ts",
    "vite.config.*",
    "eslint.config.*",
    "tsdown.config.*"
  ]
}
