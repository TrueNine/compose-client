{
  "extends": [
    "@vue/tsconfig/tsconfig.dom.json",
    "@compose/tsconfig/tsconfig.json"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.build.tsbuildinfo",
    "lib": [
      "ESNext",
      "DOM",
      "WebWorker"
    ],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": ["lodash-es"]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts"
  ]
}
