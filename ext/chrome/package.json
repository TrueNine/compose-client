{
  "name": "@compose/ext-chrome",
  "version": "0.0.21",
  "description": "chrome 扩展程序 开发库",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build": "pnpm rollup --config rollup.config.ts --configPlugin typescript",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm -C dist pub"
  },
  "peerDependencies": {
    "@types/chrome": "^0.0.243",
    "@vueuse/core": "^10.2.1",
    "moment": "^2.29.4"
  },
  "type": "module",
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./*": "./*"
  }
}
