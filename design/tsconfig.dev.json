{
  "extends": [
    "@vue/tsconfig/tsconfig.dom.json",
    "./tsconfig.app.json"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.playground.tsbuildinfo",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "./env.d.ts",
      "./.types/imports-auto.d.ts",
      "./.types/imports-comp.d.ts"
    ]
  },
  "include": [
    ".types/imports-auto.d.ts",
    ".types/imports-comp.d.ts",
    ".types/imports-typed-router.d.ts",
    "env.d.ts",
    "src/types/**.d.ts",
    "playground/**/*.ts",
    "playground/**/*.vue",
    "playground/**/*.tsx"
  ]
}
