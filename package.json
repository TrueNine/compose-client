{
  "name": "compose-client",
  "version": "1.0.8",
  "packageManager": "pnpm@10.11.0",
  "description": "百研科技前端开发包",
  "author": "TrueNine",
  "keywords": [
    "compose",
    "client",
    "ui",
    "shared",
    "ext",
    "external",
    "req",
    "psdk",
    "tmap",
    "wxpa"
  ],
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.11.0"
  },
  "scripts": {
    "lint": "eslint --fix --config ./configs/tsconfig/eslint.config.mjs",
    "build:configs:eslint9": "nr --dir ./configs/eslint9 build",
    "build:configs:vite-fragment": "nr --dir ./configs/vite-fragment build",
    "build:configs:uno": "nr --dir ./configs/uno build",
    "build:configs": "run-s build:configs:eslint9 build:configs:vite-fragment build:configs:uno",
    "build:types": "nr --dir ./types build",
    "build:shared": "nr --dir ./shared build",
    "build:shareds": "run-s build:types build:shared",
    "build:ext:chrome": "nr --dir ./ext/chrome build",
    "build:ext": "nr --dir ./ext/chrome build",
    "build:external": "nr --dir ./external build",
    "build:req": "nr --dir ./req build",
    "build:ui": "nr --dir ./ui build",
    "build:psdk:tmap": "nr --dir ./psdk/tmap build",
    "build:psdk:wxpa": "nr --dir ./psdk/wxpa build",
    "build:psdk": "run-p build:psdk:tmap build:psdk:wxpa",
    "build:apps": "run-p build:psdk build:ext build:external build:req build:ui",
    "build": "run-s build:configs build:shareds build:apps",
    "prepare1": "run-s lint build"
  },
  "devDependencies": {
    "@compose/eslint9-config": "workspace:*",
    "@compose/tsconfig": "workspace:*",
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:types",
    "eslint": "catalog:eslint",
    "vitest": "catalog:",
    "npm-run-all2": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:vite",
    "rollup": "catalog:rollup",
    "vue-tsc": "catalog:vue"
  }
}
