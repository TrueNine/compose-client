{
  "name": "compose-client",
  "version": "1.0.2",
  "author": "TrueNine",
  "description": "no desc",
  "workspaces": [
    "depd/*",
    "@types/*",
    "uni-app/*",
    "api-model"
  ],
  "scripts": {
    "lint": "eslint . --fix --ext .js,.ts,.jsx,.tsx,.vue && prettier --write --ignore-unknown .",
    "depd": "pnpm -r i",
    "build": "pnpm -r build",
    "pub": "pnpm lint && pnpm build && pnpm publish -r packages"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@compose/depd-eslint": "workspace:*",
    "@compose/depd-eslint-vue": "workspace:*",
    "@compose/depd-moment": "workspace:*",
    "@compose/depd-node": "workspace:*",
    "@compose/depd-rollup": "workspace:*",
    "@compose/depd-vite": "workspace:*"
  }
}