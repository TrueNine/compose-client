{
  "name": "compose-client",
  "version": "1.0.2",
  "author": "TrueNine",
  "description": "no desc",
  "workspaces": [
    "depd/*",
    "@types/*",
    "uni-app/*",
    "api-model"
  ],
  "scripts": {
    "lint": "eslint . --fix --ext .js,.ts,.jsx,.tsx,.vue && prettier --write --ignore-unknown .",
    "depd": "pnpm -r i",
    "build:model": "pnpm build --dir api-model",
    "pub": "pnpm lint && pnpm build && pnpm publish -r packages"
  },
  "lint-staged": {
    "*.{.js,.ts,.d.ts,.vue}": [
      "eslint . --fix --ext .js,.ts,.d.ts,.vue",
      "prettier --write --ignore-unknown --config .prettierrc.cjs ."
    ]
  },
  "devDependencies": {
    "@compose/api-model": "workspace:*",
    "@compose/eslint-config": "workspace:*",
    "@compose/prettier-config": "workspace:*",
    "@compose/tsconfig": "workspace:*",
    "@dcloudio/types": "^3.3.3",
    "@dcloudio/uni-app": "3.0.0-alpha-3080420230602001",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.4.2",
    "@types/qs": "^6.9.7",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/runtime-core": "^3.3.4",
    "@vue/runtime-dom": "^3.3.4",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "jest": "^29.6.1",
    "mini-types": "^0.1.7",
    "miniprogram-api-typings": "^3.10.0",
    "postcss": "^8.4.26",
    "rollup": "^3.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "^1.63.6",
    "terser": "^5.19.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "uni-app-types": "^0.1.7",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vue-tsc": "^1.8.5"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "vue": "^3.3.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
