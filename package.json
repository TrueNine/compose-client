{
  "name": "compose-client",
  "version": "1.0.8",
  "packageManager": "pnpm@10.11.0",
  "description": "百研科技前端开发包",
  "author": "TrueNine",
  "keywords": [
    "compose",
    "client",
    "ui",
    "shared",
    "ext",
    "external",
    "req",
    "psdk",
    "tmap",
    "wxpa"
  ],
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.11.0"
  },
  "scripts": {
    "lint": "eslint --fix --config ./configs/tsconfig/eslint.config.mjs",
    "build:configs:eslint9": "pnpm --filter ./configs/eslint9... run build",
    "build:configs:vite-fragment": "pnpm --filter ./configs/vite-fragment... run build",
    "build:configs:uno": "pnpm --filter ./configs/uno... run build",
    "build:configs": "run-s build:configs:eslint9 build:configs:vite-fragment build:configs:uno",
    "build:types": "pnpm --filter ./types... run build",
    "build:shared": "pnpm --filter ./shared... run build",
    "build:shareds": "run-s build:types build:shared",
    "build:ext:chrome": "pnpm --filter ./ext/chrome... run build",
    "build:ext": "pnpm --filter ./ext/chrome... run build",
    "build:external": "pnpm --filter ./external... run build",
    "build:req": "pnpm --filter ./req... run build",
    "build:ui": "pnpm --filter ./ui... run build",
    "build:psdk:tmap": "pnpm --filter ./psdk/tmap... run build",
    "build:psdk:wxpa": "pnpm --filter ./psdk/wxpa... run build",
    "build:psdk": "run-p build:psdk:tmap build:psdk:wxpa",
    "build:apps": "run-p build:psdk build:ext build:external build:req build:ui",
    "build": "run-s build:configs build:shareds build:apps",
    "prepare": "run-s lint build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:eslint",
    "@compose/eslint9-config": "workspace:*",
    "@compose/tsconfig": "workspace:*",
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:types",
    "eslint": "catalog:eslint",
    "npm-run-all2": "catalog:",
    "rollup": "catalog:rollup",
    "typescript": "catalog:",
    "vite": "catalog:vite",
    "vitest": "catalog:vitest",
    "vue-tsc": "catalog:vue"
  }
}
