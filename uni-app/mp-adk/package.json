{
  "name": "@compose/uni-mp-adk",
  "version": "1.0.39",
  "author": "TrueNine",
  "license": "private",
  "description": "uni-app miniProgram auxiliary development kit",
  "scripts": {
    "build": "pnpm rollup --config rollup.config.ts --configPlugin typescript",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm -C dist pub"
  },
  "type": "module",
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./*": "./*"
  }
}