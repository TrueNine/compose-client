{
  "name": "@compose/meta-ui-web",
  "version": "1.1.140",
  "scripts": {
    "dev": "vite",
    "build-gulp": "gulp -f ./__build-src__/index.ts",
    "build-vite": "vite build",
    "type-check": "vue-tsc --noEmit",
    "build": "run-s type-check build-vite build-gulp",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm publish --no-git-checks"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css",
    "*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./unplugin": {
      "types": "./dist/unplugin/index.d.ts",
      "import": "./dist/unplugin/index.js",
      "require": "./dist/unplugin/index.cjs"
    },
    "./*": "./*",
    "./**": "./**",
    "./dist/*": "./dist/*"
  }
}
