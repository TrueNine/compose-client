{
  "name": "@compose/meta-ui-web",
  "version": "1.1.22",
  "type": "module",
  "module": "dist/meta-ui-web.js",
  "main": "dist/meta-ui-web.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build-gulp": "gulp -f ./__build-src__/index.ts",
    "build-vite": "vite build",
    "build": "run-s build-vite build-gulp",
    "pub": "run-s build pub-c",
    "pub-c": "pnpm publish --no-git-checks"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  }
}
