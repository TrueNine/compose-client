{
  "name": "@compose/api-types",
  "version": "2.0.3",
  "description": "yan100 client ui kit",
  "author": "TrueNine",
  "license": "private",
  "scripts": {
    "build-c": "vite build",
    "type-check": "vue-tsc -p ./tsconfig.json --noEmit",
    "build": "run-s type-check build-c",
    "pub-c": "pnpm --dir ./dist pub",
    "lint": "eslint --fix",
    "fmt": "prettier --write \"**/*.{ts,tsx,vue,scss,d.ts,js}\" --ignore-unknown",
    "pub": "run-s fmt type-check lint build fmt pub-c"
  },
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./orm": {
      "types": "./dist/orm/index.d.ts"
    },
    "./typescripts": {
      "types": "./dist/typescripts/index.d.ts"
    },
    "./*": "./*",
    "./**": "./**",
    "./dist/*": "./dist/*",
    "./dist/**": "./dist/**"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/TrueNine",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/TrueNine/compose-client",
    "directory": "api-types"
  }
}
